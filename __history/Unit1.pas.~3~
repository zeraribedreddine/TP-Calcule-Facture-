procedure TForm1.Button1Click(Sender: TObject);
var a  :integer ;
 b ,  c : real ;
begin
 a :=  strtoint(edit1.Text) ;
  b :=  strtofloat (edit6.Text) ;
  c:= (a*b) ;
  edit7.Text := floattostr (c) ;

  //////////////////////////////////////////////
  a :=  strtoint(edit3.Text) ;
  b :=  strtofloat (edit5.Text) ;
  c:= (a*b) ;
  edit8.Text := floattostr (c) ;
  /////////////////////////////////////////////
  a :=  strtoint(edit1.Text) ;
  b :=  strtofloat (edit6.Text) ;
  c:= (a*b) ;
  edit7.Text := floattostr (c) ;
end;

